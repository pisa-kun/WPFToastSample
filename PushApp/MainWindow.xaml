<Window x:Class="PushApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PushApp"
        mc:Ignorable="d"
        Title="Push" Height="400" Width="400">
    <Grid>
        <Button Content="PushToast" Height="300" VerticalAlignment="Top" Click="Button_Click"></Button>
        <TextBox x:Name="text" Height="70" VerticalAlignment="Bottom"></TextBox>
    </Grid>
</Window>
